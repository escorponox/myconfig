{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "scss",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "graphql",
    "reason",
    "yaml",
    "markdown.mdx"
  ],
  "signature.enable": false,
  "diagnostic.enable": true,
  "diagnostic.signOffset": 1000,
  "diagnostic.errorSign": "!!",
  "diagnostic.warningSign": ">>",
  "diagnostic.infoSign": "--",
  "diagnostic.hintSign": "->",
  "diagnostic.virtualText": true,
  "diagnostic.enableMessage": "always",
  "diagnostic.maxWindowWidth": 100,
  "diagnostic.maxWindowHeight": 16,
  "coc.preferences.hoverTarget": "float",
  "javascript.suggestionActions.enabled": false,
  "list.normalMappings": {
    "t": "action:tabe",
    "s": "action:vsplit"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit"
  },
  "highlight.colors.enable": true,
  "yank.enableCompletion": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "explorer.quitOnOpen": true,
  "explorer.position": "left",
  "explorer.floating.position": "center",
  "explorer.floating.width": 80,
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.toggle": true,
  "explorer.keyMappings.global": {
    "s": "open:vsplit",
    "U": "gotoParent",
    "0": "reveal:0:compact"
  },
  "yank.highlight.enable": true
}
