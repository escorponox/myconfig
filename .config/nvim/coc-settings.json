{
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "scss",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "graphql",
    "reason",
    "markdown.mdx",
    "mdx",
    "go"
  ],
  "signature.enable": false,
  "diagnostic.enable": true,
  "diagnostic.errorSign": "!!",
  "diagnostic.warningSign": ">>",
  "diagnostic.infoSign": "--",
  "diagnostic.hintSign": "->",
  "diagnostic.virtualText": true,
  "diagnostic.enableMessage": "always",
  "javascript.suggestionActions.enabled": false,
  "list.normalMappings": {
    "t": "action:tabe",
    "s": "action:vsplit"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit"
  },
  "highlight.colors.enable": true,
  "yank.enableCompletion": false,
  "yank.highlight.enable": true,
  "sumneko-lua.enableNvimLuaDev": true,
  "diagnostic.floatConfig": {
    "maxHeight": 20
  },
  "signature.floatConfig": {
    "maxHeight": 15
  },
  "hover.floatConfig": {
    "maxHeight": 15
  },
  "suggest.floatConfig": {
    "maxHeight": 15
  },
  "Lua.telemetry.enable": true,
  "coc.source.emoji.filetypes": ["markdown", "typescriptreact"],
  "jest.customFlags": ["silent", "verbose"],
  "git.addGBlameToBufferVar": false,
  "go.goplsOptions": {
    "gofumpt": true
  }
}
